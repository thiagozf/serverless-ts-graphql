{
  "name": "serverless-ts-graphql",
  "version": "1.0.0",
  "description": "Serverless webpack example using TypeScript and Apollo",
  "main": "handler.js",
  "license": "MIT",
  "scripts": {
    "analyze": "webpack --config \"./webpack/webpack.config.bundle.js\"",
    "build": "sls webpack",
    "deploy": "yarn deploy:dev",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:staging": "sls deploy --stage staging",
    "deploy:prod": "sls deploy --stage prod",
    "pre-build": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && yarn lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint --ext \"ts,tsx,js,jsx\" .",
    "lint:fix": "eslint --ext \"ts,tsx,js,jsx\" --fix .",
    "pretest": "yarn lint",
    "start": "node --inspect $(which sls) offline",
    "start:debug": "yarn start"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.3.3",
    "class-transformer": "^0.3.1",
    "graphql": "^14.1.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "0.18.0-beta.6",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/aws-lambda": "^8.10.17",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-type-json": "^0.1.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^10.12.19",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "happypack": "^5.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "prettier": "^1.16.1",
    "serverless-domain-manager": "^3.3.0",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^4.2.0",
    "serverless-plugin-warmup": "^4.7.1-rc.1",
    "serverless-webpack": "^5.1.1",
    "source-map-support": "^0.5.6",
    "ts-jest": "^24.2.0",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.5.0",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "yamljs": "^0.3.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
